<%= @dish.name %>

<img src="<%= @dish.image_url %>" alt="">

<%= @dish.venue %>

<% @likes.each do |like| %>
  <br>
  <%= like.id %>
<% end %>

<form action="/admin/dishes/<%= @dish.id %>" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <input type="hidden" name="_method" value="delete">
  <button>delete</button>
</form>
